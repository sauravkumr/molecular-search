{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  